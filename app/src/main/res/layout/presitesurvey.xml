<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sub_background"
    android:paddingLeft="16dp"
    android:paddingRight="16dp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="441dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="91dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView293"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/job_number" />

                        <EditText
                            android:id="@+id/editText77"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <TextView
                            android:id="@+id/textView295"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/team" />

                        <TextView
                            android:id="@+id/textView297"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/pilot_in_command" />

                        <TextView
                            android:id="@+id/textView298"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/observer" />

                        <TextView
                            android:id="@+id/textView299"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/uav_registration" />

                        <TextView
                            android:id="@+id/textView300"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/helper_1" />

                        <TextView
                            android:id="@+id/textView301"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/helper_2" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="95dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView294"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/date" />

                        <EditText
                            android:id="@+id/editText78"
                            android:layout_width="75dp"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <TextView
                            android:id="@+id/textView296"
                            android:layout_width="match_parent"
                            android:layout_height="40dp" />

                        <EditText
                            android:id="@+id/editText79"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText80"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText83"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText82"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText81"
                            android:layout_width="75dp"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView302"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/site_name" />

                        <TextView
                            android:id="@+id/textView304"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/latitude_amp_longtitude" />

                        <TextView
                            android:id="@+id/textView305"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/altitude_from_sea_level" />

                        <TextView
                            android:id="@+id/textView306"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/work_required" />

                        <TextView
                            android:id="@+id/textView307"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/date_work_required" />

                        <TextView
                            android:id="@+id/textView308"
                            android:layout_width="match_parent"
                            android:layout_height="60dp"
                            android:text="@string/downloaded_map_to_groundstation" />

                        <TextView
                            android:id="@+id/textView309"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:text="@string/vehicular_acces" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <EditText
                            android:id="@+id/editText84"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText85"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText86"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:hint="@string/ft"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText87"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <EditText
                            android:id="@+id/editText88"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:ems="10"
                            android:inputType="textPersonName" />

                        <CheckBox
                            android:id="@+id/checkBox"
                            android:layout_width="match_parent"
                            android:layout_height="60dp" />

                        <RadioGroup
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                            <RadioButton
                                android:id="@+id/radioButton"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/yes" />

                            <RadioButton
                                android:id="@+id/radioButton2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/no" />
                        </RadioGroup>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>